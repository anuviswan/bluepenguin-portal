              "type": "string"
            },
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "categoryCode": {
            "type": "string",
            "nullable": true
          },
          "material": {
            "type": "string",
            "nullable": true
          },
          "featureCodes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "collectionCode": {
            "type": "string",
            "nullable": true
          },
          "yearCode": {
            "type": "integer",
            "format": "int32"
          },
          "sequenceCode": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ETag": {
        "type": "object",
        "additionalProperties": false
      },
      "MetaDataEntity": {
        "type": "object",
        "properties": {
          "partitionKey": {
            "type": "string",
            "nullable": true
          },
          "rowKey": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "eTag": {
            "$ref": "#/components/schemas/ETag"
          }
        },
        "additionalProperties": false
      },
      "SearchProductsRequest": {
        "type": "object",
        "properties": {
          "selectedCategories": {
            "type": "array",
